<div class="">
  <div class="">
    <h2>Crear cuenta</h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true,class:"" %>
      </div>

      <div class="">
        <%= f.label :ContraseÃ±a %>
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> Caracteres como minimo)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "off",class:"" %>
      </div>

      <div class="">
        <%= f.label :Confirmar_contraseÃ±a %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off",class:"" %>
      </div>

      <div class="">
        <%= f.submit "REGISTRAR",class:"" %>
      </div>
    <% end %>

    <%= render "devise/shared/links" %>
  </div>
</div>
